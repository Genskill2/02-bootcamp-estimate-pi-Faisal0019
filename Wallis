def wallis():

    print("Wallis\n===========")

    i = 1000000
    numerator = 2
    denominator = 1
    pi = 1

    for i in range(1, i + 1):
        pi*= (numerator / denominator)
        if (i%2) == 1:
            denominator+= 2
        else:
            numerator+= 2

    pi*= 2

    print_as_text(pi) 
